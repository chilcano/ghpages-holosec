<sequence xmlns="http://ws.apache.org/ns/synapse" name="ERSSubmissionToADRSRetrySequence">
   <log level="custom">
      <property name="===========" value="ERSSubmissionToADRSRetrySequence = ============"></property>
   </log>
   <log level="custom">
      <property xmlns:ns="http://org.apache.synapse/xsd" name="correlationId" expression="get-property('transport','CorrelationId')"></property>
   </log>
   <log level="custom">
      <property name="[ERSSubmissionToADRSRetrySequence]" value="Retrying to send message to ADRS Service."></property>
   </log>
   <send>
      <endpoint template="gov:/templates/ETMP_ERS_SUBMISSIONRETURN_EP_TPL.xml" uri="http://{system.prop.uk.gov.hmrc.wso2.etmp.ERS_SUBMITRETURN_HOST_NAME}:{system.prop.uk.gov.hmrc.wso2.etmp.ERS_SUBMITRETURN_HOST_PORT}/{system.prop.uk.gov.hmrc.wso2.etmp.ERS_SUBMITRETURN_EP}"></endpoint>
   </send> 
</sequence>