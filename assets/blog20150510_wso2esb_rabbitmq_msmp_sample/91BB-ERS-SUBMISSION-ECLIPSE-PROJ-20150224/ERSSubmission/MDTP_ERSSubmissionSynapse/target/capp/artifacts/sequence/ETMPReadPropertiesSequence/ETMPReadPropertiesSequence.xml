    <property name="targetSystem" expression="get-property('system','uk.gov.hmrc.wso2.etmp.esb.heartbeat.TARGET_SYSTEM')" scope="default" type="STRING" description="retrieve Target System"/>
    <property name="username" expression="get-property('system','uk.gov.hmrc.wso2.etmp.esb.username')" scope="default" type="STRING" description="retrieve username"/>
